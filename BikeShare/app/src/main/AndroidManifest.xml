<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.iastate.bikeshare" >

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage" >
        </activity>
        <activity
            android:name=".Navigation_Menu"
            android:label="@string/title_activity_navigation__menu"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".Move_Requests" >
        </activity>
        <activity android:name=".Hub_Details" >
        </activity>
        <activity android:name=".Account" >
        </activity>
        <activity android:name=".Maintenance" >
        </activity>
    </application>

</manifest>
